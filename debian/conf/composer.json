{
  "name": "spojenet/csas-statement-tools",
  "description": "ČSas (Česká spořitelna) account statement tools",
  "license": "MIT",
  "authors": [
    {
      "name": "Vítězslav Dvořák",
      "email": "info@vitexsoftware.cz"
    }
  ],
  "require": {
    "deb/ease-core": "*",
    "deb/csas-accountsapi": "*"
  },
  "autoload": {
    "psr-4": {
      "SpojeNet\\CSas\\Csas\\": "/usr/lib/csas-statement-tools/Csas/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "/usr/share/php/EaseCore"
    },
    {
      "type": "path",
      "url": "/usr/share/php/CSasAccounts"
    },
    {
      "type": "path",
      "url": "/usr/share/php/GuzzleHttp"
    }
  ]
}
